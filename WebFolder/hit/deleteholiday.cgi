<!--HIT_EXECUTE
pdelUUIDStr:=_HIT_WebApp_GetField ("holiday_uuid")If (_HIT_AuthenticateSession="Session-Authenticated-OK")
READ WRITE([employee_holidays])QUERY([employee_holidays];[employee_holidays]uuid=pdelUUIDStr)mJSONObject:=JSON New Object If (Records in selection([employee_holidays])>0)If ([employee_holidays]calender_type="company_holiday")DELETE RECORD([employee_holidays])If ([employee_holidays]uuid="")JSON Set Text (mJSONObject;"Success";"Holiday Deleted Successfull.")Else JSON Set Text (mJSONObject;"Error";"Error in Deleting, Please try Again !!")End if Else JSON Set Text (mJSONObject;"Error";"Employee Holiday Can not be Deleted !!")End if Else JSON Set Text (mJSONObject;"Error";"No Record with with this ID !!")End if 
READ ONLY([employee_holidays])Else JSON Set Text (mJSONObject;"Error";"Session Error !!")End if If (JSON Is Valid Object (mJSONObject)=1)Else JSON Set Text (mJSONObject;"Error";"Error in Deleting, Please try Again !!")End if Web_ResponseDataBLOB:=JSON Save As Blob (mJSONObject)JSON Release (mJSONObject)_HIT_HTTPD_SetHTTPHeader ("Content-Type";"application/json")_HIT_HTTPD_AppendResponse (->Web_ResponseDataBLOB)
-->